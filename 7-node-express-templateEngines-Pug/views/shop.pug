extends layouts/main-layout.pug

block styles
    link(rel="stylesheet", href="/css/product.css")

block content
    main
            if prods.length > 0 
                .grid
                    each product in prods
                        article.card.product-item
                            header.card__header
                                h1.product__title #{product.title}
                            div.card__image
                                img(src="data:image/webp;base64,", alt="A Book")
                            div.card__content
                                h2.product__price $19.99
                                p.product__description Best strategies to harness!!
                            div.card__actions
                                button.btn Add to Cart
            else 
                h1 No Products